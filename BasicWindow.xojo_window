	#tag Event
		Sub Action()
		  Dim s As String = FieldOutput.Text
		  
		  If s.Left( 1 ) = "-" Then
		    
		    FieldOutput.Text = s.Right( s.Len - 1 )
		    
		  Else
		    
		    FieldOutput.Text = "-" + s
		    
		  End If
		End Sub
